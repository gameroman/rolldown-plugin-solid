{
  "name": "rolldown-plugin-solid",
  "version": "0.2.1",
  "description": "A Rolldown plugin for compiling SolidJS JSX/TSX files using Babel presets",
  "keywords": [
    "babel",
    "bundler",
    "compiler",
    "jsx",
    "plugin",
    "rolldown",
    "solid",
    "solidjs",
    "tsx"
  ],
  "homepage": "https://github.com/g-mero/rolldown-plugin-solid#readme",
  "bugs": {
    "url": "https://github.com/g-mero/rolldown-plugin-solid/issues"
  },
  "license": "MIT",
  "author": "g-mero",
  "repository": {
    "type": "git",
    "url": "https://github.com/g-mero/rolldown-plugin-solid.git"
  },
  "files": [
    "CHANGELOG.md",
    "dist",
    "README.md"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "lint": "biome lint",
    "format": "biome check --write",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-typescript": "^7.28.5",
    "babel-plugin-jsx-dom-expressions": "^0.40.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/babel__core": "^7.20.5",
    "@types/bun": "^1.3.5",
    "@types/node": "^25.0.6",
    "solid-js": "^1.9.10",
    "tsdown": "^0.19.0"
  },
  "peerDependencies": {
    "rolldown": ">=1.0.0-beta.59",
    "solid-js": ">=1.9.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
