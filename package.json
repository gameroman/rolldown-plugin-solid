{
  "name": "rolldown-plugin-solid",
  "version": "0.2.1",
  "description": "A Rolldown plugin for compiling SolidJS JSX/TSX files using Babel presets",
  "keywords": [
    "babel",
    "bundler",
    "compiler",
    "jsx",
    "plugin",
    "rolldown",
    "solid",
    "solidjs",
    "tsx"
  ],
  "homepage": "https://github.com/g-mero/rolldown-plugin-solid#readme",
  "bugs": {
    "url": "https://github.com/g-mero/rolldown-plugin-solid/issues"
  },
  "license": "MIT",
  "author": "g-mero",
  "repository": {
    "type": "git",
    "url": "https://github.com/g-mero/rolldown-plugin-solid.git"
  },
  "files": [
    "CHANGELOG.md",
    "dist",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "biome lint",
    "format": "biome format",
    "test": "jiti ./scripts/test.ts && serve tests",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "release": "bumpp"
  },
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "babel-preset-solid": "^1.9.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^24.2.1",
    "bumpp": "^10.2.2",
    "jiti": "^2.5.1",
    "serve": "^14.2.4",
    "solid-js": "^1.9.8",
    "tsdown": "^0.13.4"
  },
  "peerDependencies": {
    "rolldown": ">=0.15.0",
    "solid-js": ">=1.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
